    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#080E3F88"
        }
      },
      {
        "scope": ["string", "punctuation.definition.string"],
        "settings": {
          "foreground": "#00525B"
        }
      },
      {
        "scope": [
          "meta.fstring.python",
          "string.quoted.double.js",
          "string.quoted.single.js",
          "string.quoted.template.js",
          "string.template",
          "string.quoted.double.ts",
          "string.quoted.single.ts",
          "string.quoted.template.ts"
        ],
        "settings": {
          "foreground": "#00525B"
        }
      },
      {
        "scope": [
          "storage.type.string.python",
          "punctuation.definition.template-expression",
          "punctuation.section.embedded",
          "meta.template.expression"
        ],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": [
          "constant.character.format.placeholder.other.python",
          "meta.template.expression",
          "variable.interpolation",
          "punctuation.definition.template-expression.begin",
          "punctuation.definition.template-expression.end"
        ],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["constant.language"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["constant.character", "constant.other"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["variable"],
        "settings": {
          "foreground": "#080E3F"
        }
      },
      {
        "scope": [
          "variable.other.object",
          "meta.attribute.python",
          "variable.other.readwrite.instance",
          "variable.other.property",
          "variable.other.object.property"
        ],
        "settings": {
          "foreground": "#080E3F"
        }
      },
      {
        "scope": ["keyword"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": [
          "keyword.control",
          "storage.type.function",
          "storage.type.class",
          "storage.type.interface",
          "storage.type.enum",
          "storage.type.struct",
          "storage.type.other",
          "keyword.declaration",
          "keyword.operator.new"
        ],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["storage"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": [
          "storage.type.primitive",
          "storage.type.built-in",
          "storage.type.other",
          "support.type"
        ],
        "settings": {
          "foreground": "#00525B"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "meta.definition.method",
          "meta.definition.function"
        ],
        "settings": {
          "foreground": "#00525B"
        }
      },
      {
        "scope": [
          "meta.function-call",
          "meta.function-call.generic",
          "meta.function-call.python",
          "meta.method-call",
          "meta.method",
          "entity.name.function.call",
          "variable.function"
        ],
        "settings": {
          "foreground": "#BA97FF"
        }
      },
      {
        "scope": [
          "entity.name.type",
          "entity.other.inherited-class",
          "support.class",
          "new.expr entity.name.type"
        ],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["entity.name.tag"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#00525B"
        }
      },
      {
        "scope": ["support.function"],
        "settings": {
          "foreground": "#BA97FF"
        }
      },
      {
        "scope": ["support.constant"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["support.type", "support.class"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["support.other.variable"],
        "settings": {
          "foreground": "#080E3F"
        }
      },
      {
        "scope": ["invalid"],
        "settings": {
          "foreground": "#BA97FF"
        }
      },
      {
        "scope": ["markup.heading"],
        "settings": {
          "foreground": "#00525B",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["markup.italic"],
        "settings": {
          "foreground": "#5A4CFF",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["markup.bold"],
        "settings": {
          "foreground": "#5A4CFF",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["markup.underline"],
        "settings": {
          "foreground": "#BA97FF",
          "fontStyle": "underline"
        }
      },
      {
        "scope": ["markup.deleted"],
        "settings": {
          "foreground": "#BA97FF"
        }
      },
      {
        "scope": ["markup.inserted"],
        "settings": {
          "foreground": "#00BEA6"
        }
      },
      {
        "scope": ["markup.changed"],
        "settings": {
          "foreground": "#5A4CFF"
        }
      },
      {
        "scope": ["punctuation.decorator"],
        "settings": {
          "foreground": "#00BEA6"
        }
      }
    ]
  }